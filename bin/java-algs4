#!/bin/bash

# *************************************************
# java-algs4
# Hayk Martirosyan
# -------------------
# Wrapper for java that includes algs4 libraries.
# *************************************************

# This must match the install directory
INSTALL=../

# Sets the path to the classpath libraries
jars=(.:${INSTALL}/lib/stdlib.jar:${INSTALL}/lib/algs4.jar)

java -cp "$jars" "$@"
